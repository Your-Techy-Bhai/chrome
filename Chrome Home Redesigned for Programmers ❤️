:root { --atom-size: 180px; --atom-color-hex: #00d8ff; --atom-color-rgb: 0, 216, 255; --nucleus-size: calc(var(--atom-size) / 5); --electron-color-hex: #99f8ff; --electron-size: calc(var(--atom-size) / 25); --electron-orbit-size: calc(var(--atom-size) / 2.5); --electron-speed: 1.2s; --electron-speed-alpha: 1s; --electron-speed-omega: .8s; }

*, ::before, ::after { box-sizing: border-box; }

.atom { position: relative; width: var(--atom-size); height: var(--atom-size); animation: 8s cubic-bezier(1, 0.25, 0, 0.75) 0s infinite normal none running atom; }

@keyframes atom { 
  0% { transform: rotate(0deg) scale(1); }
  12.5% { transform: rotate(-45deg) scale(0.9); }
  25% { transform: rotate(-90deg) scale(1); }
  37.5% { transform: rotate(-135deg) scale(0.9); }
  50% { transform: rotate(-180deg) scale(1); }
  62.5% { transform: rotate(-225deg) scale(0.9); }
  75% { transform: rotate(-270deg) scale(1); }
  87.5% { transform: rotate(-315deg) scale(0.9); }
  100% { transform: rotate(-360deg) scale(1); }
}

